<h1>My Favorite Attractions</h1>

<ol>
    {{#each attractions}}
    <li>
        <h1><a href="/attractions/{{this.id}}">{{this.fullName}}</a></h1>
        <h3>{{this.country}}</h3>
        <p>{{this.book_title}}, {{this.publication_year}}</p>
    </li>
    {{/each}}
</ol>

<a href="/authors/new" class="btn btn-default">Create a New Author</a>
